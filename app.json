      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "137b21dc-29ea-470e-a0d0-968cd72e5115"
      }
    },
    "android": {
      "package": "com.joshualikes.boltexponativewind"
    }
  }
